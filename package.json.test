        "icon": "$(trash)"
      },
      {
        "command": "snapback.deleteOlderSnapshots",
        "title": "SnapBack: Delete Older Snapshots ðŸ§¢",
        "category": "SnapBack",
        "icon": "$(clear-all)"
      },
      {
        "command": "snapback.unprotectAndDeleteSnapshot",
        "title": "SnapBack: Unprotect and Delete Snapshot ðŸ§¢",
        "category": "SnapBack"
      },
      {
        "command": "snapback.renameSnapshot",
        "title": "SnapBack: Rename Snapshot ðŸ§¢",
        "category": "SnapBack",
        "icon": "$(edit)"
      },
      {
        "command": "snapback.protectSnapshot",
        "title": "SnapBack: Protect Snapshot ðŸ§¢",
        "category": "SnapBack",
        "icon": "$(shield)"
      },
      {
        "command": "snapback.openWalkthrough",
